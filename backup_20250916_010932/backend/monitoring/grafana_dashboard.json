{
  "dashboard": {
    "id": null,
    "title": "Trae AI Production Monitoring Dashboard",
    "tags": ["trae-ai", "production", "monitoring"],
    "style": "dark",
    "timezone": "browser",
    "refresh": "30s",
    "schemaVersion": 27,
    "version": 1,
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "timepicker": {
      "refresh_intervals": ["5s", "10s", "30s", "1m", "5m", "15m", "30m", "1h", "2h", "1d"]
    },
    "templating": {
      "list": [
        {
          "name": "instance",
          "type": "query",
          "query": "label_values(up, instance)",
          "refresh": 1,
          "includeAll": true,
          "multi": true,
          "allValue": ".*"
        },
        {
          "name": "job",
          "type": "query",
          "query": "label_values(up, job)",
          "refresh": 1,
          "includeAll": true,
          "multi": true,
          "allValue": ".*"
        }
      ]
    },
    "annotations": {
      "list": [
        {
          "name": "Annotations & Alerts",
          "datasource": "prometheus",
          "enable": true,
          "expr": "ALERTS{alertstate=\"firing\"}",
          "iconColor": "rgba(255, 96, 96, 1)",
          "titleFormat": "Alert: {{alertname}}",
          "textFormat": "{{instance}}: {{summary}}"
        }
      ]
    },
    "panels": [
      {
        "id": 1,
        "title": "System Overview",
        "type": "row",
        "gridPos": {"h": 1, "w": 24, "x": 0, "y": 0},
        "collapsed": false
      },
      {
        "id": 2,
        "title": "System Health Score",
        "type": "stat",
        "gridPos": {"h": 8, "w": 6, "x": 0, "y": 1},
        "targets": [
          {
            "expr": "system_health_score",
            "legendFormat": "Health Score"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                {"color": "red", "value": 0},
                {"color": "yellow", "value": 50},
                {"color": "green", "value": 80}
              ]
            },
            "unit": "short",
            "min": 0,
            "max": 100
          }
        },
        "options": {
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"],
            "fields": ""
          },
          "orientation": "auto",
          "textMode": "auto",
          "colorMode": "value",
          "graphMode": "area",
          "justifyMode": "auto"
        }
      },
      {
        "id": 3,
        "title": "CPU Usage",
        "type": "timeseries",
        "gridPos": {"h": 8, "w": 6, "x": 6, "y": 1},
        "targets": [
          {
            "expr": "system_cpu_usage_percent",
            "legendFormat": "CPU Usage %"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            },
            "unit": "percent",
            "min": 0,
            "max": 100,
            "thresholds": {
              "steps": [
                {"color": "green", "value": 0},
                {"color": "yellow", "value": 70},
                {"color": "red", "value": 90}
              ]
            }
          }
        },
        "options": {
          "tooltip": {
            "mode": "single",
            "sort": "none"
          },
          "legend": {
            "displayMode": "visible",
            "placement": "bottom"
          }
        }
      },
      {
        "id": 4,
        "title": "Memory Usage",
        "type": "timeseries",
        "gridPos": {"h": 8, "w": 6, "x": 12, "y": 1},
        "targets": [
          {
            "expr": "(system_memory_usage_bytes{type=\"used\"}/system_memory_usage_bytes{type=\"total\"}) * 100",
            "legendFormat": "Memory Usage %"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            },
            "unit": "percent",
            "min": 0,
            "max": 100,
            "thresholds": {
              "steps": [
                {"color": "green", "value": 0},
                {"color": "yellow", "value": 80},
                {"color": "red", "value": 95}
              ]
            }
          }
        }
      },
      {
        "id": 5,
        "title": "Disk Usage",
        "type": "timeseries",
        "gridPos": {"h": 8, "w": 6, "x": 18, "y": 1},
        "targets": [
          {
            "expr": "(system_disk_usage_bytes{type=\"used\"}/system_disk_usage_bytes{type=\"total\"}) * 100",
            "legendFormat": "Disk Usage % - {{device}}"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            },
            "unit": "percent",
            "min": 0,
            "max": 100,
            "thresholds": {
              "steps": [
                {"color": "green", "value": 0},
                {"color": "yellow", "value": 85},
                {"color": "red", "value": 95}
              ]
            }
          }
        }
      },
      {
        "id": 6,
        "title": "Application Metrics",
        "type": "row",
        "gridPos": {"h": 1, "w": 24, "x": 0, "y": 9},
        "collapsed": false
      },
      {
        "id": 7,
        "title": "Request Rate",
        "type": "timeseries",
        "gridPos": {"h": 8, "w": 8, "x": 0, "y": 10},
        "targets": [
          {
            "expr": "rate(requests_total[5m])",
            "legendFormat": "Requests/sec - {{method}} {{status}}"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            },
            "unit": "reqps"
          }
        }
      },
      {
        "id": 8,
        "title": "Error Rate",
        "type": "timeseries",
        "gridPos": {"h": 8, "w": 8, "x": 8, "y": 10},
        "targets": [
          {
            "expr": "rate(errors_total[5m])",
            "legendFormat": "Errors/sec - {{error_type}}"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            },
            "unit": "short",
            "thresholds": {
              "steps": [
                {"color": "green", "value": 0},
                {"color": "yellow", "value": 5},
                {"color": "red", "value": 20}
              ]
            }
          }
        }
      },
      {
        "id": 9,
        "title": "Response Time (95th percentile)",
        "type": "timeseries",
        "gridPos": {"h": 8, "w": 8, "x": 16, "y": 10},
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(request_duration_seconds_bucket[5m]))",
            "legendFormat": "95th percentile"
          },
          {
            "expr": "histogram_quantile(0.50, rate(request_duration_seconds_bucket[5m]))",
            "legendFormat": "50th percentile"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            },
            "unit": "s"
          }
        }
      },
      {
        "id": 10,
        "title": "Web Scraping Metrics",
        "type": "row",
        "gridPos": {"h": 1, "w": 24, "x": 0, "y": 18},
        "collapsed": false
      },
      {
        "id": 11,
        "title": "Scraping Success Rate",
        "type": "stat",
        "gridPos": {"h": 8, "w": 6, "x": 0, "y": 19},
        "targets": [
          {
            "expr": "(rate(scraping_requests_total{status=\"success\"}[10m])/rate(scraping_requests_total[10m])) * 100",
            "legendFormat": "Success Rate %"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                {"color": "red", "value": 0},
                {"color": "yellow", "value": 80},
                {"color": "green", "value": 95}
              ]
            },
            "unit": "percent",
            "min": 0,
            "max": 100
          }
        }
      },
      {
        "id": 12,
        "title": "Scraping Duration",
        "type": "timeseries",
        "gridPos": {"h": 8, "w": 9, "x": 6, "y": 19},
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(scraping_duration_seconds_bucket[5m]))",
            "legendFormat": "95th percentile - {{source}}"
          },
          {
            "expr": "histogram_quantile(0.50, rate(scraping_duration_seconds_bucket[5m]))",
            "legendFormat": "50th percentile - {{source}}"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            },
            "unit": "s"
          }
        }
      },
      {
        "id": 13,
        "title": "Scraping Requests by Source",
        "type": "timeseries",
        "gridPos": {"h": 8, "w": 9, "x": 15, "y": 19},
        "targets": [
          {
            "expr": "rate(scraping_requests_total[5m])",
            "legendFormat": "{{source}} - {{status}}"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            },
            "unit": "reqps"
          }
        }
      },
      {
        "id": 14,
        "title": "API Discovery Metrics",
        "type": "row",
        "gridPos": {"h": 1, "w": 24, "x": 0, "y": 27},
        "collapsed": false
      },
      {
        "id": 15,
        "title": "APIs Discovered",
        "type": "stat",
        "gridPos": {"h": 8, "w": 6, "x": 0, "y": 28},
        "targets": [
          {
            "expr": "increase(api_discovery_total{status=\"validated\"}[24h])",
            "legendFormat": "APIs Discovered (24h)"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                {"color": "red", "value": 0},
                {"color": "yellow", "value": 10},
                {"color": "green", "value": 50}
              ]
            },
            "unit": "short"
          }
        }
      },
      {
        "id": 16,
        "title": "API Validation Success Rate",
        "type": "timeseries",
        "gridPos": {"h": 8, "w": 9, "x": 6, "y": 28},
        "targets": [
          {
            "expr": "(rate(api_discovery_total{status=\"validated\"}[10m])/rate(api_discovery_total[10m])) * 100",
            "legendFormat": "Validation Success Rate % - {{source}}"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            },
            "unit": "percent",
            "min": 0,
            "max": 100
          }
        }
      },
      {
        "id": 17,
        "title": "API Validation Duration",
        "type": "timeseries",
        "gridPos": {"h": 8, "w": 9, "x": 15, "y": 28},
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(api_validation_duration_seconds_bucket[5m]))",
            "legendFormat": "95th percentile - {{source}}"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            },
            "unit": "s"
          }
        }
      },
      {
        "id": 18,
        "title": "Cache & Performance",
        "type": "row",
        "gridPos": {"h": 1, "w": 24, "x": 0, "y": 36},
        "collapsed": false
      },
      {
        "id": 19,
        "title": "Cache Hit Rate",
        "type": "stat",
        "gridPos": {"h": 8, "w": 6, "x": 0, "y": 37},
        "targets": [
          {
            "expr": "(rate(cache_operations_total{result=\"hit\"}[10m])/rate(cache_operations_total[10m])) * 100",
            "legendFormat": "Cache Hit Rate %"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                {"color": "red", "value": 0},
                {"color": "yellow", "value": 60},
                {"color": "green", "value": 85}
              ]
            },
            "unit": "percent",
            "min": 0,
            "max": 100
          }
        }
      },
      {
        "id": 20,
        "title": "Cache Operations",
        "type": "timeseries",
        "gridPos": {"h": 8, "w": 9, "x": 6, "y": 37},
        "targets": [
          {
            "expr": "rate(cache_operations_total[5m])",
            "legendFormat": "{{operation}} - {{result}}"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            },
            "unit": "ops"
          }
        }
      },
      {
        "id": 21,
        "title": "Active Connections",
        "type": "timeseries",
        "gridPos": {"h": 8, "w": 9, "x": 15, "y": 37},
        "targets": [
          {
            "expr": "active_connections",
            "legendFormat": "{{type}} connections"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            },
            "unit": "short"
          }
        }
      },
      {
        "id": 22,
        "title": "Business Metrics",
        "type": "row",
        "gridPos": {"h": 1, "w": 24, "x": 0, "y": 45},
        "collapsed": false
      },
      {
        "id": 23,
        "title": "Daily Revenue",
        "type": "stat",
        "gridPos": {"h": 8, "w": 6, "x": 0, "y": 46},
        "targets": [
          {
            "expr": "business_revenue_daily",
            "legendFormat": "Revenue ({{currency}})"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                {"color": "red", "value": 0},
                {"color": "yellow", "value": 100},
                {"color": "green", "value": 500}
              ]
            },
            "unit": "currencyUSD"
          }
        }
      },
      {
        "id": 24,
        "title": "User Activity Events",
        "type": "timeseries",
        "gridPos": {"h": 8, "w": 9, "x": 6, "y": 46},
        "targets": [
          {
            "expr": "rate(user_activity_events[5m])",
            "legendFormat": "{{event_type}} events/sec"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            },
            "unit": "short"
          }
        }
      },
      {
        "id": 25,
        "title": "Data Quality Score",
        "type": "timeseries",
        "gridPos": {"h": 8, "w": 9, "x": 15, "y": 46},
        "targets": [
          {
            "expr": "data_quality_score",
            "legendFormat": "Quality Score - {{dataset}}"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            },
            "unit": "percent",
            "min": 0,
            "max": 100,
            "thresholds": {
              "steps": [
                {"color": "red", "value": 0},
                {"color": "yellow", "value": 70},
                {"color": "green", "value": 90}
              ]
            }
          }
        }
      },
      {
        "id": 26,
        "title": "Queue & Processing",
        "type": "row",
        "gridPos": {"h": 1, "w": 24, "x": 0, "y": 54},
        "collapsed": false
      },
      {
        "id": 27,
        "title": "Queue Size",
        "type": "timeseries",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 55},
        "targets": [
          {
            "expr": "queue_size",
            "legendFormat": "{{queue_name}} queue size"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            },
            "unit": "short",
            "thresholds": {
              "steps": [
                {"color": "green", "value": 0},
                {"color": "yellow", "value": 500},
                {"color": "red", "value": 2000}
              ]
            }
          }
        }
      },
      {
        "id": 28,
        "title": "Automation Success Rate",
        "type": "timeseries",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 55},
        "targets": [
          {
            "expr": "automation_success_rate",
            "legendFormat": "{{automation_type}} success rate"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            },
            "unit": "percent",
            "min": 0,
            "max": 100,
            "thresholds": {
              "steps": [
                {"color": "red", "value": 0},
                {"color": "yellow", "value": 80},
                {"color": "green", "value": 95}
              ]
            }
          }
        }
      }
    ]
  }
}