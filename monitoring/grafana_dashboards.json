{
  "dashboard": {
    "id": null,
    "title": "TRAE AI - Real-time Performance & Auto-scaling Dashboard",
    "tags": ["trae-ai", "performance", "auto-scaling", "monitoring"],
    "style": "dark",
    "timezone": "browser",
    "panels": [
      {
        "id": 1,
        "title": "System Overview",
        "type": "stat",
        "targets": [
          {
            "expr": "system_cpu_usage_percent",
            "legendFormat": "CPU Usage"
          },
          {
            "expr": "system_memory_usage_percent",
            "legendFormat": "Memory Usage"
          },
          {
            "expr": "sum(service_health_status)",
            "legendFormat": "Healthy Services"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                {
                  "color": "green",
                  "value": null
                },
                {
                  "color": "yellow",
                  "value": 70
                },
                {
                  "color": "red",
                  "value": 90
                }
              ]
            },
            "unit": "percent"
          }
        },
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 0
        }
      },
      {
        "id": 2,
        "title": "HTTP Request Rate",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(http_requests_total[5m])",
            "legendFormat": "{{method}} {{endpoint}}"
          }
        ],
        "yAxes": [
          {
            "label": "Requests/sec",
            "min": 0
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 0
        }
      },
      {
        "id": 3,
        "title": "Response Time Distribution",
        "type": "graph",
        "targets": [
          {
            "expr": "histogram_quantile(0.50, rate(http_request_duration_seconds_bucket[5m]))",
            "legendFormat": "50th percentile"
          },
          {
            "expr": "histogram_quantile(0.95, rate(http_request_duration_seconds_bucket[5m]))",
            "legendFormat": "95th percentile"
          },
          {
            "expr": "histogram_quantile(0.99, rate(http_request_duration_seconds_bucket[5m]))",
            "legendFormat": "99th percentile"
          }
        ],
        "yAxes": [
          {
            "label": "Response Time (s)",
            "min": 0
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 24,
          "x": 0,
          "y": 8
        }
      },
      {
        "id": 4,
        "title": "Model Generation Performance",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(model_generation_requests_total{status=\"success\"}[5m])",
            "legendFormat": "Successful Generations/sec"
          },
          {
            "expr": "rate(model_generation_requests_total{status=\"error\"}[5m])",
            "legendFormat": "Failed Generations/sec"
          },
          {
            "expr": "model_generation_queue_size",
            "legendFormat": "Queue Size"
          }
        ],
        "yAxes": [
          {
            "label": "Count",
            "min": 0
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 16
        }
      },
      {
        "id": 5,
        "title": "Model Generation Duration",
        "type": "graph",
        "targets": [
          {
            "expr": "histogram_quantile(0.50, rate(model_generation_duration_seconds_bucket[5m]))",
            "legendFormat": "50th percentile"
          },
          {
            "expr": "histogram_quantile(0.95, rate(model_generation_duration_seconds_bucket[5m]))",
            "legendFormat": "95th percentile"
          }
        ],
        "yAxes": [
          {
            "label": "Duration (s)",
            "min": 0
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 16
        }
      },
      {
        "id": 6,
        "title": "Resource Utilization for Auto-scaling",
        "type": "graph",
        "targets": [
          {
            "expr": "resource_utilization_percent{resource_type=\"cpu\"}",
            "legendFormat": "CPU - {{service}}"
          },
          {
            "expr": "resource_utilization_percent{resource_type=\"memory\"}",
            "legendFormat": "Memory - {{service}}"
          }
        ],
        "yAxes": [
          {
            "label": "Utilization %",
            "min": 0,
            "max": 100
          }
        ],
        "thresholds": [
          {
            "value": 70,
            "colorMode": "critical",
            "op": "gt"
          },
          {
            "value": 90,
            "colorMode": "critical",
            "op": "gt"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 24,
          "x": 0,
          "y": 24
        }
      },
      {
        "id": 7,
        "title": "Scaling Events",
        "type": "graph",
        "targets": [
          {
            "expr": "increase(scaling_events_total{action=\"scale_up\"}[5m])",
            "legendFormat": "Scale Up - {{resource_type}}"
          },
          {
            "expr": "increase(scaling_events_total{action=\"scale_down\"}[5m])",
            "legendFormat": "Scale Down - {{resource_type}}"
          }
        ],
        "yAxes": [
          {
            "label": "Events",
            "min": 0
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 32
        }
      },
      {
        "id": 8,
        "title": "Service Health Status",
        "type": "stat",
        "targets": [
          {
            "expr": "service_health_status",
            "legendFormat": "{{service_name}}"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                {
                  "color": "red",
                  "value": 0
                },
                {
                  "color": "green",
                  "value": 1
                }
              ]
            },
            "mappings": [
              {
                "options": {
                  "0": {
                    "text": "Unhealthy"
                  },
                  "1": {
                    "text": "Healthy"
                  }
                },
                "type": "value"
              }
            ]
          }
        },
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 32
        }
      },
      {
        "id": 9,
        "title": "Database Performance",
        "type": "graph",
        "targets": [
          {
            "expr": "database_connections_active",
            "legendFormat": "Active Connections"
          },
          {
            "expr": "histogram_quantile(0.95, rate(database_query_duration_seconds_bucket[5m]))",
            "legendFormat": "95th percentile query time"
          }
        ],
        "yAxes": [
          {
            "label": "Connections/Time (s)",
            "min": 0
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 40
        }
      },
      {
        "id": 10,
        "title": "Cache Performance",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(cache_hits_total[5m])",
            "legendFormat": "Cache Hits/sec - {{cache_type}}"
          },
          {
            "expr": "rate(cache_misses_total[5m])",
            "legendFormat": "Cache Misses/sec - {{cache_type}}"
          },
          {
            "expr": "rate(cache_hits_total[5m])/(rate(cache_hits_total[5m]) + rate(cache_misses_total[5m])) * 100",
            "legendFormat": "Hit Rate % - {{cache_type}}"
          }
        ],
        "yAxes": [
          {
            "label": "Rate/Percentage",
            "min": 0
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 40
        }
      },
      {
        "id": 11,
        "title": "Disk Usage by Mount Point",
        "type": "graph",
        "targets": [
          {
            "expr": "system_disk_usage_percent",
            "legendFormat": "{{mount_point}}"
          }
        ],
        "yAxes": [
          {
            "label": "Usage %",
            "min": 0,
            "max": 100
          }
        ],
        "thresholds": [
          {
            "value": 80,
            "colorMode": "critical",
            "op": "gt"
          },
          {
            "value": 95,
            "colorMode": "critical",
            "op": "gt"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 24,
          "x": 0,
          "y": 48
        }
      }
    ],
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "timepicker": {
      "refresh_intervals": [
        "5s",
        "10s",
        "30s",
        "1m",
        "5m",
        "15m",
        "30m",
        "1h",
        "2h",
        "1d"
      ]
    },
    "templating": {
      "list": [
        {
          "name": "service",
          "type": "query",
          "query": "label_values(service_health_status, service_name)",
          "refresh": 1,
          "includeAll": true,
          "multi": true
        },
        {
          "name": "resource_type",
          "type": "query",
          "query": "label_values(resource_utilization_percent, resource_type)",
          "refresh": 1,
          "includeAll": true,
          "multi": true
        }
      ]
    },
    "annotations": {
      "list": [
        {
          "name": "Scaling Events",
          "datasource": "Prometheus",
          "expr": "scaling_events_total",
          "titleFormat": "Scaling Event: {{action}}",
          "textFormat": "Resource: {{resource_type}}",
          "iconColor": "blue"
        }
      ]
    },
    "refresh": "30s",
    "schemaVersion": 27,
    "version": 1,
    "links": [
      {
        "title": "Prometheus",
        "url": "http://localhost:9090",
        "type": "link"
      },
      {
        "title": "Application Health",
        "url": "http://localhost:8080/health",
        "type": "link"
      }
    ]
  }
}